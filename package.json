{
  "name": "react-clockwall",
  "version": "1.4.7",
  "description": "Digital Clocks made with ReactJS",
  "repository": {
    "type": "git",
    "url": "https://github.com/amine7536/clockwall"
  },
  "keywords": [
    "gulp",
    "react",
    "react-component",
    "browserify",
    "seed",
    "boilerplate",
    "kickstarter",
    "ES6",
    "ES2015",
    "babel",
    "mocha",
    "chai"
  ],
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive",
    "coverage": "babel-node ./node_modules/.bin/babel-istanbul cover --root src/ ./node_modules/.bin/_mocha -- --recursive",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "watch": "gulp watch",
    "build": "gulp build",
    "build-docs": "./node_modules/.bin/jsdoc -c jsdoc.json -t ./node_modules/ink-docstrap/template --readme README.md",
    "build-lib": "babel --out-dir lib/ src/components/"
  },
  "main": "lib/Clock.js",
  "author": "pixelfactory.io",
  "license": "MIT",
  "dependencies": {
    "babel-istanbul": "^0.12.1",
    "moment": "^2.10.6",
    "moment-timezone": "^0.5.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1"
  },
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.2.0",
    "browser-sync": "^2.10.1",
    "browserify": "14.4.0",
    "chai": "4.1.1",
    "coveralls": "^2.11.8",
    "cssnano": "^3.0.0",
    "eslint": "^3.19.0 || ^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "estraverse": "^4.2.0",
    "estraverse-fb": "^1.3.1",
    "gulp": "^3.9.0",
    "gulp-eslint": "4.0.0",
    "gulp-html-replace": "^1.5.1",
    "gulp-notify": "3.0.0",
    "gulp-postcss": "7.0.0",
    "gulp-sourcemaps": "2.6.1",
    "gulp-uglify": "3.0.0",
    "ink-docstrap": "^1.1.0",
    "jsdoc": "^3.4.0",
    "mocha": "^10.2.0",
    "postcss-nested": "2.1.2",
    "postcss-simple-extend": "^1.0.0",
    "postcss-simple-vars": "4.1.0",
    "react-addons-test-utils": "^15.0.1",
    "rimraf": "^2.5.0",
    "run-sequence": "2.1.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.4.0"
  }
